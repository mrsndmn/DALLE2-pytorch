{
    "prior": {
        "net": {
            "dim": 512,
            "depth": 12,
            "num_timesteps": 1000,
            "max_text_len": 77,
            "num_time_embeds": 1,
            "num_image_embeds": 1,
            "num_text_embeds": 1,
            "dim_head": 64,
            "heads": 12,
            "ff_mult": 4,
            "norm_out": true,
            "attn_dropout": 0.05,
            "ff_dropout": 0.05,
            "final_proj": true,
            "normformer": true,
            "rotary_emb": true
        },
        "image_embed_dim": 512,
        "image_size": 224,
        "image_channels": 3,
        "timesteps": 1000,
        "sample_timesteps": 64,
        "cond_drop_prob": 0.1,
        "loss_type": "l2",
        "predict_x_start": true,
        "beta_schedule": "cosine",
        "condition_on_text_encodings": false
    },
    "data": {
        "batch_size": 256,
        "num_data_points": 100000,
        "eval_every_seconds": 3600,
        "image_url": "../data/audiocaps_prior_train_embeddings/audio",
        "txt_url": "../data/audiocaps_prior_train_embeddings/text",
        "meta_url": "../data/audiocaps_prior_train_embeddings/metadata.jsonl",
        "splits": {
            "train": 0.8,
            "val":  0.1,
            "test": 0.1
        }
    },
    "train": {
        "epochs": 250,
        "lr": 1.1e-4,
        "wd": 6.02e-2,
        "max_grad_norm": 0.5,
        "use_ema": true,
        "ema_beta": 0.9999,
        "ema_update_after_step": 50,
        "warmup_steps": 50,
        "amp": false,
        "save_every_seconds": 3600,
        "eval_timesteps": [64, 1000],
        "random_seed": 84513
    },
    "tracker": {
        "data_path": ".prior",
        "overwrite_data_path": true,
        "log": {
            "log_type": "wandb",
            "wandb_entity": "hsemrsndmn",
            "wandb_project": "prior_debugging",
            "wandb_resume": false,
            "verbose": true
        },
        "save": [
            {
                "save_to": "local",
                "save_type": "checkpoint",
                "save_latest_to": ".prior/latest_checkpoint.pth",
                "save_best_to": ".prior/best_checkpoint.pth"
            }
        ]
    }
}
