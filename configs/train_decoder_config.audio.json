{
    "decoder": {
        "unets": [
            {
                "dim": 128,
                "image_embed_dim": 512,
                "cond_dim": 64,
                "channels": 1,
                "dim_mults": [1, 2, 4, 8],
                "attn_dim_head": 32,
                "attn_heads": 16,
                "self_attn": [false, true, true, true]
            }
        ],
        "image_sizes": [ [16, 128] ],
        "channels": 1,
        "timesteps": 1000,
        "loss_type": "l2",
        "beta_schedule": ["cosine"],
        "learned_variance": true
    },
    "data": {
        "webdataset_base_url": "../data/audiocaps_train_embeddings_1k/webdataset-{}.tar",
        "num_workers": 2,
        "batch_size": 64,
        "start_shard": 0,
        "end_shard": 0,
        "shard_width": 4,
        "index_width": 4,
        "splits": {
            "train": 0.75,
            "val": 0.15,
            "test": 0.1
        },
        "shuffle_train": true,
        "resample_train": false,
        "preprocessing": {
            "ToTensor": true
        }
    },
    "train": {
        "epochs": 1000,
        "lr": 1e-4,
        "wd": 0.01,
        "max_grad_norm": 0.5,
        "save_every_n_samples": 100000,
        "n_sample_images": 6,
        "device": "cuda:0",
        "epoch_samples": null,
        "validation_samples": null,
        "use_ema": true,
        "ema_beta": 0.99,
        "amp": false,
        "unet_training_mask": [true]
    },
    "evaluate": {
        "n_evaluation_samples": 1000,
        "FID": {
            "feature": 64
        },
        "IS": {
            "feature": 64,
            "splits": 10
        },
        "KID": {
            "feature": 64,
            "subset_size": 10
        },
        "LPIPS": {
            "net_type": "vgg",
            "reduction": "mean"
        }
    },
    "tracker": {
        "overwrite_data_path": true,
        "data_path": ".decoder",

        "log": {
            "log_type": "console",
            "wandb_entity": "hsemrsndmn",
            "wandb_project": "dalle2_decoder",
            "verbose": true
        },

        "load": {
            "load_from": "local",
            "file_path": ".decoder/best_checkpoint_500e.pth"
        },

        "save": [
            {
                "save_to": "local",
                "save_type": "checkpoint",
                "save_latest_to": ".decoder/latest_checkpoint.pth",
                "save_best_to": ".decoder/best_checkpoint.pth"
            }
        ]
    }
}
